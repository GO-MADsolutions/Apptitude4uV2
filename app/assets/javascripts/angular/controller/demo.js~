var demoApp = angular.module("demo", []);

demoApp.controller("democ", function($scope)
{
   $scope.message = "DEMO TEST";
});




demoApp.config(function($routeProvider)
{
$routeProvider
.when('/ages', {
   templateUrl : '../app/views/ages/index.html.erb',
   controller : 'routedemo'
});
});

demoApp.controller("routedemo", function($scope)
{
    console.log("WORKING");
   $scope.message = "DEMO TEST ROUTE";
});
